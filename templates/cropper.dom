<div class='cropper'>
    {{#if model.src }}
        <div class='cropper-mask'>
            <img src="{{model.src}}">
        </div>
        <input name="scale" type="range" min="{{model.minScale}}" max="{{model.maxScale}}" step="0.1" value="{{model.scale}}">
    {{#else }}
        No image, upload one.
    {{/if}}
</div>
